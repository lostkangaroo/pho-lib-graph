{
    "name": "phonetworks/pho-lib-graph",
    "type": "library",
    "description": "A general purpose graph library in PHP",
    "keywords": [
        "graph",
        "network",
        "node",
        "vertex",
        "edge",
        "predicate"
    ],
    "homepage": "https://phonetworks.org",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Emre Sokullu",
            "email": "emre@phonetworks.org",
            "homepage": "http://www.emresokullu.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/log": "*",
        "ramsey/uuid": "^4.7.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpdocumentor/phpdocumentor": "^3.0",
        "psy/psysh": "*",
        "fzaninotto/faker": "dev-master",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "3.*",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Pho\\Lib\\Graph\\": "src/Pho/Lib/Graph/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pho\\Lib\\Graph\\": "tests/Pho/Lib/Graph/"
        }
    }
}
